@namespace MudBlazor.Docs.Examples

<MudTimeline>
    <MudTimelineItem>
        <MudText Typo="Typo.h6">Order Received</MudText>
    </MudTimelineItem>
    <MudTimelineItem Color="Color.Success">
        <MudText Typo="Typo.h6" Align="Align.End">Payment Received</MudText>
    </MudTimelineItem>
    <MudTimelineItem Color="Color.Info">
        <MudText Typo="Typo.h6">Package Process</MudText>
    </MudTimelineItem>
    <MudTimelineItem SkipOrder="true">
        <MudText Typo="Typo.body2" Class="mud-text-secondary">packaging complete<br />8:30am</MudText>
    </MudTimelineItem>
    <MudTimelineItem>
        <MudText Typo="Typo.body2" Class="mud-text-secondary">waiting for pickup<br />9:00am</MudText>
    </MudTimelineItem>
    <MudTimelineItem SkipOrder="true">
        <MudText Typo="Typo.body2" Class="mud-text-secondary">package picked up by driver<br />10:00am</MudText>
    </MudTimelineItem>
    <MudTimelineItem SkipOrder="true" Color="Color.Success">
        <MudText Typo="Typo.h6" Align="Align.End">Package In Transit</MudText>
    </MudTimelineItem>
    <MudTimelineItem>
        <MudText Typo="Typo.body2" Align="Align.End" Class="mud-text-secondary">arrived at pennybridge postal center<br />12:00pm</MudText>
    </MudTimelineItem>
</MudTimeline>